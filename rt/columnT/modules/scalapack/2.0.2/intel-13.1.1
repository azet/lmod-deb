#%Module1.0
#####################################################################

set modulename  scalapack
set version	2.0.2
set compiler	intel-13.1.1
set basepath	/opt/scalapack/$version/intel-13.1.1

proc ModulesHelp { } {
    puts stderr "\tLoads $modulename ($version)"
}

module-whatis "Loads $modulename ($version), compiler $compiler."

# Requirements for the module:
prereq intel/13.1.1
prereq openmpi/1.6.4/intel-13.1.1

