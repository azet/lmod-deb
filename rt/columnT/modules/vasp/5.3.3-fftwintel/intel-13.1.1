#%Module1.0
#####################################################################

set modulename  vasp
set version	5.3.3-fftwintel
set compiler	intel-13.1.1
set basepath	/opt/vasp/$version/intel-13.1.1

proc ModulesHelp { } {
    puts stderr "\tLoads $modulename ($version)"
}

module-whatis "Loads $modulename ($version), compiler $compiler."

# Requirements for the module:
prereq intel/13.1.1
prereq fftw/intel/intel-13.1.1
prereq openmpi/1.6.4/intel-13.1.1
prereq wannier90/1.2/intel-13.1.1

prepend-path PATH             $basepath/bin
