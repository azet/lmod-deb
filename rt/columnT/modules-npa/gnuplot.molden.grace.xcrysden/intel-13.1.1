#%Module1.0
#####################################################################

set modulename  "Nick Papior Andersen's script for loading GUI: intel-13.1.1"
set version	1.2
set compiler	intel-13.1.1
set basepath	/directory/should/not/exist

proc ModulesHelp { } {
    puts stderr "\tLoads $modulename ($version)"
}

module-whatis "Loads $modulename ($version), compiler $compiler."

# This module will load the following modules:
module load intel/13.1.1
module load gnuplot/4.6.1/intel-13.1.1
module load zlib/1.2.7/intel-13.1.1
module load hdf5-serial/1.8.10/intel-13.1.1
module load netcdf-serial/4.2.1.1/intel-13.1.1
module load fftw/2.1.5/intel-13.1.1
module load fftw/3.3.2/intel-13.1.1
module load molden/5.0/intel-13.1.1
module load grace/5.1.23/intel-13.1.1
module load xcrysden/1.5.53/intel-13.1.1

