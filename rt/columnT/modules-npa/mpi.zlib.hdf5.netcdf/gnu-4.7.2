#%Module1.0
#####################################################################

set modulename  "Nick Papior Andersen's module script for: gnu-4.7.2"
set version	13-150
set compiler	gnu-4.7.2
set basepath	/directory/should/not/exist

proc ModulesHelp { } {
    puts stderr "\tLoads $modulename ($version)"
}

module-whatis "Loads $modulename ($version), compiler $compiler."

# This module will load the following modules:
module load openmpi/1.6.4/gnu-4.7.2
module load zlib/1.2.7/gnu-4.7.2
module load hdf5/1.8.10/gnu-4.7.2
module load pnetcdf/1.3.1/gnu-4.7.2
module load netcdf/4.2.1.1/gnu-4.7.2

