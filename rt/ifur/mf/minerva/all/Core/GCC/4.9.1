#%Module

proc ModulesHelp { } {
    puts stderr {   The GNU Compiler Collection includes front ends for C, C++, Objective-C, Fortran, Java, and Ada,
 as well as libraries for these languages (libstdc++, libgcj,...). - Homepage: http://gcc.gnu.org/
    }
}

module-whatis {Description: The GNU Compiler Collection includes front ends for C, C++, Objective-C, Fortran, Java, and Ada,
 as well as libraries for these languages (libstdc++, libgcj,...). - Homepage: http://gcc.gnu.org/}

set root    /csc/minerva/2.1/software/Core/GCC/4.9.1
set mproot           "MODULEPATH_ROOT"
set modulepath_root  $env($mproot)
conflict    GCC
#module use /csc/minerva/2.1/modules/all/Compiler/GCC/4.9.1
module use "$modulepath_root/minerva/all/Compiler/GCC/4.9.1"


prepend-path	CPATH		$root/include
prepend-path	LD_LIBRARY_PATH		$root/lib
prepend-path	LD_LIBRARY_PATH		$root/lib64
prepend-path	LD_LIBRARY_PATH		$root/lib/gcc/x86_64-unknown-linux-gnu/4.9.1
prepend-path	LIBRARY_PATH		$root/lib
prepend-path	LIBRARY_PATH		$root/lib64
prepend-path	MANPATH		$root/share/man
prepend-path	PATH		$root/bin

setenv	EBROOTGCC		"$root"
setenv	EBVERSIONGCC		"4.9.1"
setenv	EBDEVELGCC		"$root/easybuild/Core-GCC-4.9.1-easybuild-devel"


