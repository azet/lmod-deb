#%Module

proc ModulesHelp { } {
    puts stderr {   FCM is a set of tools for managing and building source code. - Homepage: http://www.metoffice.gov.uk/research/collaboration/fcm
}
}

module-whatis {FCM is a set of tools for managing and building source code. - Homepage: http://www.metoffice.gov.uk/research/collaboration/fcm}

set root    /home/xyzzy/.local/easybuild/software/FCM/2.3.1

conflict    FCM

prepend-path	LD_LIBRARY_PATH		$root/lib
prepend-path	MANPATH		$root/man
prepend-path	PATH		$root/bin
