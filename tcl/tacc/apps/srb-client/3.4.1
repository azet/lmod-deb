#%Module#####################################################################
# TACC local environment module file     $Date: 20.4.5/09/24 21:27:11 $
#
# This module file appends the relevant paths in /usr/local to the end
# of $PATH and $MANPATH. If you want the local software suite to be searched
# before the Cray-supplied paths, unload the `TACC' module and load the
# `local' module in your .login_user file.
#
# Do not edit this file directly as it is under RCS control.
#
#############################################################################
proc ModulesHelp { } {
puts stderr "The srb-client modulefile defines the default paths and environment"
puts stderr "variables needed to execute the TeraGrid tgcp utility available"
puts stderr "in /share/apps/teragrid/srb-client, placing them before the vendor-supplied"
puts stderr "paths in PATH and MANPATH.:"
}

module-whatis "Name: SRB-Client"
module-whatis "Version: 3.4.1"
module-whatis "Category: grid"
module-whatis "URL: http://software.teragrid.org/install.htm"
module-whatis "Description: TeraGrid SRB Client Software Package"



setenv       	   SRB_HOME        /share/apps/teragrid/srb-client-3.4.1-r1
prepend-path       PATH            /share/apps/teragrid/srb-client-3.4.1-r1/bin
prepend-path       MANPATH         /share/apps/teragrid/srb-client-3.4.1-r1/man
prepend-path       LD_LIBRARY_PATH /share/apps/teragrid/srb-client-3.4.1-r1/lib

module load globus/4.0.1



